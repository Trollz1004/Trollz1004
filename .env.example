# YouAndINotAI - Environment Variables Template
# SECURITY: Copy this to .env and fill in your actual values
# NEVER commit .env to version control

# Application
NODE_ENV=production
PORT=8080
DOMAIN=youandinotai.com
APP_URL=https://youandinotai.com

# Database (Cloud SQL)
DATABASE_URL=postgresql://username:password@/dbname?host=/cloudsql/PROJECT_ID:REGION:INSTANCE_NAME
DB_HOST=
DB_PORT=5432
DB_NAME=youandinotai
DB_USER=youandinotai_user
DB_PASSWORD=

# Redis (Memorystore)
REDIS_HOST=
REDIS_PORT=6379
REDIS_PASSWORD=

# Square Payments (LIVE - NOT SANDBOX)
SQUARE_ACCESS_TOKEN=
SQUARE_LOCATION_ID=
SQUARE_APP_ID=
SQUARE_BASIC_PLAN_ID=
SQUARE_PREMIUM_PLAN_ID=
SQUARE_ELITE_PLAN_ID=

# Google Gemini AI
GEMINI_API_KEY=

# Azure Cognitive Services
AZURE_FACE_ENDPOINT=
AZURE_FACE_KEY=
AZURE_COMPUTER_VISION_ENDPOINT=
AZURE_COMPUTER_VISION_KEY=

# JWT Authentication
JWT_SECRET=
JWT_REFRESH_SECRET=
JWT_EXPIRATION=24h
JWT_REFRESH_EXPIRATION=7d

# Email (Gmail SMTP)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
EMAIL_FROM=

# Cloudflare
CLOUDFLARE_ZONE_ID=
CLOUDFLARE_API_TOKEN=
CLOUDFLARE_ACCOUNT_ID=

# GCP
GCP_PROJECT_ID=spring-asset-476800-u6
GCP_REGION=us-central1
GCP_BUCKET=

# API Keys (Optional Features)
REPLICATE_API_KEY=
HUGGINGFACE_API_KEY=
OPENAI_API_KEY=
STABILITY_AI_KEY=

# Monitoring & Logging
LOG_LEVEL=info
SENTRY_DSN=

# Feature Flags
ENABLE_AI_SAFETY_AGENT=true
ENABLE_DATE_CONCIERGE=true
ENABLE_RELATIONSHIP_COACH=true
ENABLE_TRUST_SCORE=true
