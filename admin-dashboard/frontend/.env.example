# ============================================================================
# ADMIN DASHBOARD FRONTEND - ENVIRONMENT CONFIGURATION
# Domain: youandinotai.online
# ============================================================================

# API Configuration
VITE_API_URL=https://youandinotai.online/api
VITE_API_BASE_PATH=/api
VITE_WEBSOCKET_URL=https://youandinotai.online

# WebSocket Configuration
VITE_SOCKET_PATH=/socket.io
VITE_ENABLE_SOCKET_IO=true

# Application Configuration
VITE_APP_NAME=Trollz1004 Admin Dashboard
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production
VITE_DOMAIN=youandinotai.online

# Date App Integration
VITE_DATE_APP_URL=https://youandinotai.com
VITE_DATE_APP_API=https://youandinotai.com/api

# Theme Configuration
VITE_THEME=dark
VITE_PRIMARY_COLOR=#ff9d00
VITE_ENABLE_LIGHT_MODE=false

# Feature Flags
VITE_ENABLE_AI_ORCHESTRATION=true
VITE_ENABLE_CONTENT_CREATION=true
VITE_ENABLE_VIDEO_EDITOR=true
VITE_ENABLE_DAO_MANAGEMENT=true
VITE_ENABLE_TREASURY=true
VITE_ENABLE_GRANTS=true
VITE_ENABLE_FUNDRAISING=true
VITE_ENABLE_SOCIAL_POSTING=true
VITE_ENABLE_COMET_BROWSER=true
VITE_ENABLE_FILE_BROWSER=true
VITE_ENABLE_MONITORING=true
VITE_ENABLE_AUTO_RESOLVE=true

# AI Provider Display
VITE_SHOW_CLAUDE=true
VITE_SHOW_GEMINI=true
VITE_SHOW_PERPLEXITY=true
VITE_SHOW_OLLAMA=true
VITE_SHOW_WEBUI=true
VITE_SHOW_OPENAI=true

# Media Configuration
VITE_MAX_VIDEO_SIZE_MB=500
VITE_MAX_IMAGE_SIZE_MB=50
VITE_SUPPORTED_VIDEO_FORMATS=mp4,webm,mov
VITE_SUPPORTED_IMAGE_FORMATS=jpg,jpeg,png,gif,webp

# Video Editor Configuration
VITE_ENABLE_FFMPEG_WASM=true
VITE_FFMPEG_CORE_URL=https://unpkg.com/@ffmpeg/core@0.12.6/dist/umd

# Real-time Updates
VITE_REFRESH_INTERVAL_MS=5000
VITE_HEALTH_CHECK_INTERVAL_MS=30000
VITE_METRICS_UPDATE_INTERVAL_MS=10000

# Google OAuth (for date app)
VITE_GOOGLE_CLIENT_ID=your-google-oauth-client-id.apps.googleusercontent.com

# Analytics
VITE_ENABLE_ANALYTICS=true
VITE_ANALYTICS_ID=

# Security
VITE_ENABLE_2FA=true
VITE_SESSION_TIMEOUT_MIN=120

# Monitoring Display
VITE_SHOW_CPU_METRICS=true
VITE_SHOW_RAM_METRICS=true
VITE_SHOW_DISK_METRICS=true
VITE_SHOW_NETWORK_METRICS=true
VITE_SHOW_AGENT_METRICS=true

# Debug Mode
VITE_DEBUG=false
VITE_LOG_LEVEL=info
VITE_SHOW_DEV_TOOLS=false

# Content Distribution
VITE_ENABLE_URL_DISTRIBUTION=true
VITE_VALIDATE_URLS=true

# Social Media Platforms
VITE_SHOW_TWITTER=true
VITE_SHOW_INSTAGRAM=true
VITE_SHOW_FACEBOOK=true
VITE_SHOW_REDDIT=true
VITE_SHOW_TIKTOK=true
VITE_SHOW_LINKEDIN=true
VITE_SHOW_YOUTUBE=true

# Blockchain Display
VITE_SHOW_ETHEREUM=true
VITE_SHOW_POLYGON=true
VITE_SHOW_ARBITRUM=true
VITE_SHOW_BITCOIN=true
VITE_SHOW_SOLANA=true

# UI Configuration
VITE_ITEMS_PER_PAGE=20
VITE_MAX_AGENTS_DISPLAY=100
VITE_ENABLE_ANIMATIONS=true
VITE_ENABLE_SOUND_EFFECTS=false
